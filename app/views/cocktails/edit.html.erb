<h1>Editing cocktail</h1>

<%= simple_form_for [@cocktail] do |f| %>
  <%= f.input :name %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit "Submit", class: "btn btn-primary" %>
<% end %>
<%#= render 'form', cocktail: @cocktail %>

<p><%= link_to "Back to cocktails", cocktails_path %></p>
